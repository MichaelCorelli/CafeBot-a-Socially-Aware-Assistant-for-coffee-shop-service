<?xml version="1.0"?>
<robot name="cornetto">

  <material name="cornetto_color">
    <color rgba="0.73 0.34 0 1.0" />
  </material>

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="center">
    <visual>
      <origin xyz="0 0 0.025"/>
      <geometry>
        <cylinder radius="0.025" length="0.05"/>
      </geometry>
      <material name="cornetto_color"/>
    </visual>

    <collision>
      <origin xyz="0 0 0.025"/>
      <geometry>
        <cylinder radius="0.025" length="0.05"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0.025"/>
      <mass value="0.03"/>
      <inertia ixx="0.0001" ixy="0" ixz="0"
               iyy="0.0001" iyz="0"
               izz="0.0001"/>
    </inertial>
  </link>

  <link name="left">
    <visual>
      <origin xyz="-0.035 0 0.025"/>
      <geometry>
        <cylinder radius="0.018" length="0.04"/>
      </geometry>
      <material name="cornetto_color"/>
    </visual>

    <collision>
      <origin xyz="-0.035 0 0.025"/>
      <geometry>
        <cylinder radius="0.018" length="0.04"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="-0.035 0 0.025"/>
      <mass value="0.02"/>
      <inertia ixx="0.00005" ixy="0" ixz="0"
               iyy="0.00005" iyz="0"
               izz="0.00005"/>
    </inertial>
  </link>

  <link name="right">
    <visual>
      <origin xyz="0.035 0 0.025"/>
      <geometry>
        <cylinder radius="0.018" length="0.04"/>
      </geometry>
      <material name="cornetto_color"/>
    </visual>

    <collision>
      <origin xyz="0.035 0 0.025"/>
      <geometry>
        <cylinder radius="0.018" length="0.04"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.035 0 0.025"/>
      <mass value="0.02"/>
      <inertia ixx="0.00005" ixy="0" ixz="0"
               iyy="0.00005" iyz="0"
               izz="0.00005"/>
    </inertial>
  </link>

  <link name="end_2">
    <visual>
      <origin xyz="-0.045 0.015 0.025" rpy="0 0 0.4"/>
      <geometry>
        <cylinder radius="0.006" length="0.03"/>
      </geometry>
      <material name="cornetto_color"/>
    </visual>

    <collision>
      <origin xyz="-0.045 0.015 0.025" rpy="0 0 0.4"/>
      <geometry>
        <cylinder radius="0.006" length="0.03"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="-0.045 0.015 0.025"/>
      <mass value="0.01"/>
      <inertia ixx="0.00001" ixy="0" ixz="0"
               iyy="0.00001" iyz="0"
               izz="0.00001"/>
    </inertial>
  </link>

  <link name="end_1">
    <visual>
      <origin xyz="0.045 0.015 0.025" rpy="0 0 -0.4"/>
      <geometry>
        <cylinder radius="0.006" length="0.03"/>
      </geometry>
      <material name="cornetto_color"/>
    </visual>

    <collision>
      <origin xyz="0.045 0.015 0.025" rpy="0 0 -0.4"/>
      <geometry>
        <cylinder radius="0.006" length="0.03"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.045 0.015 0.025"/>
      <mass value="0.01"/>
      <inertia ixx="0.00001" ixy="0" ixz="0"
               iyy="0.00001" iyz="0"
               izz="0.00001"/>
    </inertial>
  </link>

  <link name="plate_bottom">
  <visual>
    <origin xyz="0 0 0"/>
    <geometry>
      <cylinder radius="0.05" length="0.01"/>
    </geometry>
    <material name="white"/>
  </visual>
  <collision>
    <origin xyz="0 0 0"/>
    <geometry>
      <cylinder radius="0.05" length="0.01"/>
    </geometry>
  </collision>
  <inertial>
    <origin xyz="0 0 0"/>
    <mass value="0.05"/>
    <inertia ixx="0.0001" ixy="0" ixz="0"
             iyy="0.0001" iyz="0"
             izz="0.0001"/>
  </inertial>
</link>

<link name="plate_top">
  <visual>
    <origin xyz="0 0 0.012"/>
    <geometry>
      <cylinder radius="0.1" length="0.004"/>
    </geometry>
    <material name="white"/>
  </visual>
  <collision>
    <origin xyz="0 0 0.012"/>
    <geometry>
      <cylinder radius="0.1" length="0.004"/>
    </geometry>
  </collision>
  <inertial>
    <origin xyz="0 0 0"/>
    <mass value="0.03"/>
    <inertia ixx="0.00005" ixy="0" ixz="0"
             iyy="0.00005" iyz="0"
             izz="0.00005"/>
  </inertial>
</link>

  <joint name="center_to_left" type="fixed">
    <parent link="center"/>
    <child link="left"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name="center_to_right" type="fixed">
    <parent link="center"/>
    <child link="right"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name="left_to_end_2" type="fixed">
    <parent link="left"/>
    <child link="end_2"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name="right_to_end_1" type="fixed">
    <parent link="right"/>
    <child link="end_1"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name="plate_joint" type="fixed">
  <parent link="plate_bottom"/>
  <child link="plate_top"/>
  <origin xyz="0 0 0"/>
</joint>

<joint name="plate_to_cornetto" type="fixed">
  <parent link="plate_top"/>
  <child link="center"/>
  <origin xyz="0 0 0"/>
</joint>


</robot>
